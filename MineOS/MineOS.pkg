--–ö–æ–Ω—Å—Ç–∞–Ω—Ç—ã –ø—Ä–æ–≥—Ä–∞–º–º—ã
local constants = {
	OCIFSignature = "OCIF",
	OCIF2Elements = {
		alphaStart = "A",
		symbolStart = "S",
		backgroundStart = "B",
		foregroundStart = "F",
	},
