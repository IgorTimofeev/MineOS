	fh:write("IHDR") -- name of chunk
	fh:write(outbuf.str) -- chunk data
	__write_msb_uint32(fh, CRC32Lua.crc32_string("IHDR" .. outbuf.str)) -- chunk data CRC32
	outbuf.str = "" -- reset buffer
	
