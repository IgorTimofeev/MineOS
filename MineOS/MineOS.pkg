      removeEmptyNodes(vnode)
      return true
    end
  end
  local address = type(fsOrPath) == "table" and fsOrPath.address or fsOrPath
  local result = false
  for proxy, path in filesystem.mounts() do
    local addr = type(proxy) == "table" and proxy.address or proxy
