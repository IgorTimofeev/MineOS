function rayEngine.drawDebugInformation(x, y, width, transparency, ...)
	local lines = {...}
	buffer.square(x, y, width, #lines, 0x000000, 0x000000, " ", transparency); x = x + 1
	for line = 1, #lines do buffer.text(x, y, 0xEEEEEE, lines[line]); y = y + 1 end
