	local topID, centerID, bottomID = 1, 1, 1
	for i = 1, 3 do
		points[i][2] = math.floor(points[i][2])
		if points[i][2] < points[topID][2] then topID = i end
		if points[i][2] > points[bottomID][2] then bottomID = i end
	end
