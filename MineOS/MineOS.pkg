    command = name,
    env = env,
    data = setmetatable(
    {
      handles = {},
      io = setmetatable({}, {__index=p and p.data and p.data.io or nil}),
      coroutine_handler = setmetatable({}, {__index=p and p.data and p.data.coroutine_handler or nil}),
