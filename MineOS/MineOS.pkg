	local dataType = type(data)
	if dataType == "table" then
		error("Matrix by matrix division doesn't supported yet")
	elseif dataType == "number" then
		for y = 1, #matrix do
			for x = 1, #matrix[y] do
				matrix[y][x] = matrix[y][x] / data
			end
		end

